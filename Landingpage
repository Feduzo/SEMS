<script>
  // <![CDATA[
   

  // Clear cart on page load
  $(document).ready(function(){
    $.ajax({
        type: "POST",
        url: '/cart/clear.js',
        data: '',
        dataType: 'json',
        success: function() { 
          console.log("clear");
        },
        error: function(XMLHttpRequest, textStatus) {
          console.log(XMLHttpRequest);
          console.log(textStatus);
        }
      });
    });
   
  
  $(document).on("click", ".buy-button-checkout", function(e){
    e.preventDefault();
    var code = ""
    if($(this).hasClass('buy-button-plus-one')){
      var items = [
        { quantity: 3, id: 41466552418485 },
        { quantity: 1, id: 41466552418485 }
      ];
       if(items[0].quantity === 3) {
                code = "PromoV6Page";
            }

    
    } else if($(this).hasClass('buy-button-two')){
      var items = [
        { quantity: 2, id: 41466552418485 }
      ];
       if(items[0].quantity === 2) {
                code = "";
            }

     
    } else if($(this).hasClass('buy-button-one')){
      var items = [
        { quantity: 1, id: 41466552418485 }
      ];
 if(items[0].quantity === 1) {
                code = "";
            }
     
    }
        $.post('/cart/add.js', {
            items: items
          }).done(function() {
            window.location = '/checkout?discount='+code;
          });
      
  });

   
</script>


 

